{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": ["error", "never", { "js": "always" }],
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
    },
    "settings": {
        "import/core-modules": ["got"] // lmao??
    }
}
